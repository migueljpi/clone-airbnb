<h1>PLACEHOLDER INDEX</h1>
<%= link_to "log in", new_user_session_path %>
<%= link_to "sign up", new_user_registration_path %>
<%= link_to "log out", destroy_user_session_path, data: { turbo_method: :delete } %>

<p>
  <% if user_signed_in? %>
    logged in as <%= current_user.first_name %>
    <%= link_to "My Account", user_path(current_user) %>
  <% else %>
    not logged in
  <% end %>
</p>

<% @tours.each do |tour| %>
  <p><%= link_to tour.name, tour_path(tour) %></p>
<% end %>

<h1>THESE ARE ALL THE USERS JUST FOR TESTING PURPOSES (we won't need them here)</h1>
<% @users.each do |user| %>
  <ul>
    <%= link_to user.first_name, user_path(user) %>
  </ul>
<% end %>
